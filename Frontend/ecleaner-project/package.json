{
  "name": "ecleaner-project",
  "version": "0.0.1",
  "description": "A system for cleaners",
  "productName": "eCleaner App",
  "author": "eCleaner team",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,vue,html,md,json}\" --ignore-path .gitignore",
    "test": "vitest run",
    "dev": "quasar dev",
    "build": "quasar build",
    "build:debug": "cross-env BUILD_DEBUG=true quasar build",
    "build:obfuscated": "quasar build",
    "build:cloudflare": "cross-env BUILD_CLOUDFLARE=true quasar build && echo 'Build otimizado para Cloudflare Pages criado em dist/spa (com minificacao)'",
    "serve": "quasar serve dist/spa --history",
    "serve:debug": "quasar serve dist/spa --history --port 8090",
    "start": "http-server dist/spa -p 8080 -c-1",
    "start:debug": "http-server dist/spa -p 8090 -c-1",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@canvas-fonts/arial": "^1.0.4",
    "@emailjs/browser": "^4.4.1",
    "@fontsource/montserrat": "^5.2.8",
    "@infectoone/vue-ganttastic": "^2.3.2",
    "@quasar/extras": "^1.16.4",
    "dayjs": "^1.11.19",
    "http-server": "^14.1.1",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "pinia": "^3.0.1",
    "quasar": "^2.16.0",
    "vue": "3.5.20",
    "vue-i18n": "^11.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@quasar/app-vite": "^2.1.0",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "autoprefixer": "^10.4.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.14",
    "prettier": "^3.3.3",
    "vite-plugin-checker": "^0.9.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
